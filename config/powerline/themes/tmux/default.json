{
  "default_module": "powerline.segments.common",
  "segment_data": {
    "uptime": {
      "before": "⇑  "
    },
    "external_ip": {
      "before": "ⓦ "
    },
    "date": {
      "before": "⌚ "
    }
  },
  "segments": {
    "right": [
    {
      "name": "uptime",
      "priority": 50
    },
    {
      "name": "external_ip",
      "priority": 50
    },
    {
      "name": "weather",
      "priority": 50,
      "args": {
        "unit": "C",
        "location_query": "sao_paulo, brazil"
      }
    },
    {
      "name": "system_load",
      "priority": 50
    },
    {
      "name": "date"
    },
    {
      "name": "date",
      "args": {
        "format": "%H:%M",
        "istime": true
      }
    }
    ]
  }
}
